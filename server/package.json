{
  "name": "server",
  "version": "0.1.0",
  "description": "Microservice for mobile application",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon src/index.ts",
    "checkup": "yarn run prettier:fix && yarn run eslint:fix",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "eslint:run": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@koa/cors": "^3.1",
    "@types/jest": "^26.0.13",
    "@types/koa": "^2.11",
    "@types/koa-bodyparser": "^4.3",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-passport": "^4.0",
    "@types/koa-router": "^7.4",
    "@types/koa-session": "^5.10",
    "@types/koa__cors": "^3.0",
    "@types/passport-local": "^1.0",
    "@types/ramda": "^0.27.14",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cross-env": "^7.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "nodemon": "^2.0",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "~4.0",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "koa": "^2.13",
    "koa-bodyparser": "^4.3",
    "koa-compose": "^4.1.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.1",
    "koa-router": "^9.4",
    "koa-session": "^6.0",
    "log4js": "^6.3",
    "passport-local": "^1.0",
    "ramda": "^0.27.1",
    "uuid": "^8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
